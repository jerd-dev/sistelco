<%= form_with model: @user do |form| %>

    <div>
        <%= form.label :nombre %>
        <%= form.text_field :nombre %>
    </div>

    <div>
        <%= form.label :dni %>
        <%= form.text_field :dni %>
    </div>

    <div>
        <%= form.label :'Tipo Persona' %>
        <%= form.text_field :type_person %>
    </div>

    <div>
        <%= form.label :'fecha emision' %>
        <%= form.text_field :fecha_emision %>
    </div>

    <div>
        <%= form.label :'fecha vencimiento' %>
        <%= form.text_field :fecha_vencimiento %>
    </div>

    <div>
        <%= form.label :email %>
        <%= form.text_field :email %>
    </div>

    <div>
        <%= form.label :'Telefono Principal' %>
        <%= form.number_field :telefono_p %>
    </div>

    <div>
        <%= form.label :'Telefono Secundario' %>
        <%= form.number_field :telefono_s %>
    </div>

    <%= form.submit %>
<% end %>